[DEFAULT]


[agent]
tunnel_types = vxlan,gre
l2_population = True
extensions = qos
arp_responder = true


[ovs]
bridge_mappings = physnet1:br-provider
local_ip = {{ DATA_IP }}
datapath_type = system


[securitygroup]
firewall_driver = neutron.agent.linux.iptables_firewall.OVSHybridIptablesFirewallDriver

